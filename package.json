{
  "name": "@tenjojeremy/webpack-config",
  "version": "3.78.0",
  "description": "Webpack Setup",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Tenjo/webpack-config.git"
  },
  "author": "Jeremy Tenjo <tenjojeremy@gmail.com>",
  "bugs": {
    "url": "https://gitlab.com/Tenjo/webpack-config/issues"
  },
  "homepage": "https://gitlab.com/Tenjo/webpack-config#readme",
  "scripts": {
    "start": "webpack-dev-server --config ./configs/config.project.js --mode development --hot --inline",
    "build": "webpack --config ./configs/config.project.js -p",
    "build:library": "webpack --config ./configs/config.library.js -p",
    "build:analyze": "webpack --config ./configs/config.project.js -p && webpack-bundle-analyzer --port 4200 build/stats.json",
    "deploy:firebase": "yarn build && firebase deploy",
    "deploy": "yarn publish --minor && gpush",
    "publish:mayor": "yarn publish --mayor && gpush",
    "pretty": "prettier \"**/*.js\" --write",
    "lint": "eslint ./",
    "test": "jest",
    "check-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint --fix-dry-run", "prettier --list-different", "git add"]
  },
  "bin": {
    "webpack-scripts": "./bin/webpack-scripts.js"
  },
  "browserslist": ["> 0.5%", "last 2 versions", "not dead"],
  "sideEffects": false,
  "devDependencies": {
    "@tenjojeremy/eslint-config": "1.0.0",
    "react-hot-loader": "^4.11.1",
    "styled-components": "^4.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.0.0",
    "get-my-local-ip": "^0.1.16",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "html-webpack-template": "^6.2.0",
    "husky": "^2.4.1",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.2.1",
    "markdown-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^1.18.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-test-renderer": "^16.8.3",
    "robotstxt-webpack-plugin": "^5.0.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.0"
  }
}
