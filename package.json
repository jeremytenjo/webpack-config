{
  "name": "@tenjo/webpack-config",
  "version": "3.24.0",
  "description": "Webpack Setup",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Tenjo/webpack-config.git"
  },
  "author": "Jeremy Tenjo <tenjojeremy@gmail.com>",
  "bugs": {
    "url": "https://gitlab.com/Tenjo/webpack-config/issues"
  },
  "homepage": "https://gitlab.com/Tenjo/webpack-config#readme",
  "scripts": {
    "start": "webpack-dev-server --config ./configs/config.project.js --mode development --hot --inline",
    "build": "webpack --config ./configs/config.project.js -p",
    "build:library": "webpack --config ./configs/config.library.js -p",
    "build:analyze": "webpack --config ./configs/config.project.js -p && webpack-bundle-analyzer --port 4200 build/stats.json",
    "deploy": "yarn lint && yarn test && yarn publish --minor && gpush",
    "publish:mayor": "yarn publish --mayor && gpush",
    "pretty": "prettier \"**/*.js\" --write",
    "lint": "eslint ./",
    "test": "jest",
    "check-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix-dry-run",
      "prettier --list-different",
      "git add"
    ]
  },
  "bin": {
    "webpack-scripts": "./bin/webpack-scripts.js"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@tenjo/eslint-config": "^0.7.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^3.0.1",
    "get-my-local-ip": "^0.1.16",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "markdown-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-test-renderer": "^16.8.3",
    "robotstxt-webpack-plugin": "^5.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
